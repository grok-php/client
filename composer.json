{
    "name": "grok-php/client",
    "description": "Grok PHP Client is a robust and community-driven PHP client library for seamless integration with Grok AI API, offering efficient access to advanced AI and data processing capabilities.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/grok-php/client",
    "support": {
        "issues": "https://github.com/grok-php/client/issues",
        "source": "https://github.com/grok-php/client"
    },
    "autoload": {
        "psr-4": {
            "GrokPHP\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrokPHP\\Client\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Grok PHP",
            "email": "thefeqy@gmail.com",
            "role": "organization"
        },
        {
            "name": "Muhammed Elfeqy",
            "email": "thefeqy@gmail.com",
            "homepage": "https://github.com/thefeqy",
            "role": "creator"
        }
    ],
    "keywords": [
        "Grok AI",
        "AI API",
        "PHP AI SDK",
        "Generative AI",
        "Machine Learning",
        "Natural Language Processing",
        "Large Language Model",
        "NLP",
        "AI Integration",
        "AI Client",
        "AI SDK",
        "REST API",
        "AI Text Generation",
        "Deep Learning",
        "Artificial Intelligence",
        "API Wrapper",
        "Text Processing",
        "AI-powered Applications",
        "AI Chatbot",
        "Language Model",
        "AI Research",
        "AI Developer Tools"
    ],
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^11"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:types": "phpstan analyse src --ansi",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
